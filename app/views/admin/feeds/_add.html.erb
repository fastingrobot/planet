<div class="form">
  <% form_for :feed, :url => admin_feeds_path, :method => :post do |feed| %>
  <h3>Add A Feed</h3>
  <div class="form_container">
    <dl>
      <dt>Feed URL:</dt>
      <dd><%= feed.text_field :url %></dd>
    </dl>
    <div class="operations">
      <%= submit_tag "Add Feed" %> or <%= link_to_function "Cancel", "new Effect.BlindUp('add')" %>
    </div>
  </div>
  <% end %>
</div>